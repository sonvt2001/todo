 =>{\n    const newTodos = [...todos]\n    newTodos.forEach(todo =>{\n      todo.complete = !checkAll\n    })\n    setTodos(newTodos)\n    setCheckAll(!checkAll)\n  }\n\n  const deleteTodo =() =>{\n    const newTodos = todos.filter(todo => {\n      return todo.complete === false\n    })\n    setTodos(newTodos)\n    setCheckAll(false)\n  }\n\n  return (\n    <>\n    {\n      todos.length === 0 ? <h5>Congratulations! \n        <br/>You have nothing to do today!</h5> :\n      <div className=\"row\">\n        <label htmlFor=\"all\">\n        <input type=\"checkbox\" name=\"all\" id=\"all\"\n          onChange={handleCheckAll} checked={checkAll}/>All\n        </label>\n        <p>You have ({todos.length}) things to do!</p>\n        <button id=\"delete\" onClick={deleteTodo}>\n          <i class=\"fa fa-trash-o\" aria-hidden=\"true\"></i>\n        </button>\n      </div>\n    }\n    </> \n  )\n}\n","import React from 'react';\nimport FormInput from './components/FormInput';\nimport Listtodo from './components/Listtodo';\nimport Footer from './components/Footer';\nimport {Data} from './components/Data'\n\nfunction App() {\n\n  // const[openModal, setOpenModal] = useState(false)\n  return (\n    <Data>\n    <div className=\"App\">\n      <h1>To Do List</h1>\n      <FormInput/>\n      <Listtodo/>\n      <Footer/>\n    </div>\n    </Data>\n  );\n}\n\nexport default App;\n","import React from 'react';\r\nimport {Routes, Route, Link, BrowserRouter} from 'react-router-dom';\r\nimport {Data} from './components/Data'\r\nimport ModalPage from './components/Modal';\r\nimport TodoPage from './App';\r\nimport './Nav.css'\r\nfunction Nav() {\r\n\r\n  return (\r\n  <BrowserRouter>\r\n    <nav className='navLinks'>\r\n      <ul className='links'>\r\n        <li className='link-item'>\r\n          <Link to=\"/\" className='link'>Todos</Link>\r\n        </li>\r\n        <li className='link-item'>\r\n          <Link to=\"/profile\" className='link'>Profile</Link>\r\n        </li>\r\n      <